{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 120,
	"tabWidth": 4,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"semi": false,
	"bracketSameLine": true,
	"endOfLine": "lf",
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
